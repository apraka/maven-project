pipeline {
    agent any
    stages {
        stage ('SCM Checkout') {
		  steps {
            		git 'https://github.com/apraka/maven-project.git'
		  }
        }
        
        stage ('Compile Stage') {
            steps {
                    sh 'mvn clean compile'
            }
        }

        stage ('packaging') {
            steps {
                    sh 'mvn package'
            }
        }
	}
}
